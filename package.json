{
  "name": "yasp",
  "description": "Open source Dota data platform",
  "version": "6.0.0",
  "license": "GPL-3.0",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "NODE_ENV=test test/run.sh",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "ncu": "npm-check-updates -a",
    "webpack": "webpack -p --bail",
    "watch": "webpack --watch",
    "mocha": "NODE_ENV=test mocha",
    "build": "npm install && npm run webpack",
    "rediskeys": "redis-cli keys '*' | cut -d':' -f1 | sort | uniq -c",
    "resetpicks": "redis-cli keys 'picks_*' | xargs redis-cli del",
    "resetrankings": "redis-cli keys 'hero_rankings*' | xargs redis-cli del",
    "dockerbuild": "sudo docker build -t yasp/yasp .",
    "reparse": "cat matches.csv | xargs -n1 node tasks/getMatch.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/yasp-dota/yasp"
  },
  "dependencies": {
    "JSONStream": "^1.1.4",
    "async": "2.0.1",
    "big-number": "0.4.0",
    "body-parser": "1.15.2",
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "braintree": "^1.41.0",
    "bull": "1.0.0",
    "c3": "0.4.11",
    "cal-heatmap": "3.6.0",
    "cassandra-driver": "^3.1.1",
    "chance": "^1.0.4",
    "compression": "^1.6.2",
    "connect-multiparty": "^2.0.0",
    "connect-redis": "^3.1.0",
    "cookie-session": "^2.0.0-alpha.1",
    "datatables": "^1.10.12",
    "datatables-bootstrap3-plugin": "^0.5.0",
    "dota2": "^4.0.0",
    "dota2-minimap-hero-sprites": "^2.0.0",
    "dotaconstants": "yasp-dota/dotaconstants#931dcae",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "flag-icon-css": "^2.3.0",
    "font-awesome": "^4.6.3",
    "heatmap.js": "2.0.5",
    "http-proxy": "^1.14.0",
    "jade": "^1.11.0",
    "jquery": "^3.1.0",
    "knex": "^0.11.9",
    "moment": "^2.14.1",
    "multer": "^1.2.0",
    "numeral": "^1.5.3",
    "passport": "0.3.2",
    "passport-steam": "1.0.5",
    "pg": "^6.0.3",
    "pg-query-stream": "^1.0.0",
    "pm2": "^1.1.3",
    "qtip2": "3.0.3",
    "redis": "2.6.2",
    "request": "2.74.0",
    "request-progress": "2.0.1",
    "select2": "^4.0.3",
    "select2-bootstrap-theme": "0.1.0-beta.8",
    "sentiment": "2.0.0",
    "steam": "1.4.0",
    "stripe": "4.10.0",
    "wordcloud": "1.0.3"
  },
  "devDependencies": {
    "coveralls": "2.11.12",
    "css-loader": "^0.24.0",
    "database-cleaner": "^1.0.0",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "istanbul": "^0.4.4",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.1",
    "nock": "^8.0.0",
    "npm-check-updates": "2.8.0",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  }
